.testimonials.testimonials-v2 .item p:after {
	width: 0;
	height: 0;
	content: " ";
	display: block;
	position: absolute;
	border-top-color: #f5f5f5;
	border-left-style: inset; /*FF fixes*/
	border-right-style: inset; /*FF fixes*/
}

