/* Model window popup center of the screen */
.modal {text-align: center;padding: 0!important;}
.modal:before {content: '';display: inline-block;height: 100%;vertical-align: middle;margin-right: -4px;}
.modal-dialog {display: inline-block;text-align: left;vertical-align: middle;}
.modal-body{max-height: 400px;overflow: auto;padding: 15px 25px;}


/*Headings or Typography styles here*/
h1 {font-size: 28px;line-height: 35px;}
h2 {font-size: 24px;line-height: 33px;}
h3 {font-size: 20px;line-height: 27px;}
h4 {line-height: 25px;}
h5 {line-height: 20px;}
h6 {line-height: 18px;}

/*Paragraph styles here*/
p{line-height:24px;font-size:14px;}

/*Hyperlink styles here*/
a,a:focus,a:hover,a:active,button,button:hover {outline: 0 !important;}
a:focus {text-decoration: none;}
a{color:#0a5498;text-decoration: none;}
a:hover{text-decoration: underline;}

/* Custom Form fields styles here*/
.form-control{border-radius:5px;}
input.form-control{border-radius:5px;}
input.form-control:focus{border: 1px solid #00AEC7;}
select.form-control{border-radius:5px;}
select.form-control:focus{border: 1px solid #00AEC7;}
label.error{color:#DA291C;}

.input-group .form-control:last-child,.input-group .form-control:first-child{border-radius:5px;}
.radius-left-top-bot{border-radius: 5px 0 0 5px !important;}
.radius-right-top-bot{border-radius: 0 5px 5px 0 !important;}

.table-bordered>tbody>tr>td, .table>thead>tr>th{height:40px;}

/* Custom Radio button styles here */
input[type="radio"] {float: left;width: 28px;opacity: 0;height: 12px;filter: alpha(opacity = 0);-khtml-opacity: 0;-moz-opacity: 0;left: -3px;z-index: 1;position:relative;}
input[type="radio"] + label {display: inline-block;float: left;position: relative;padding-top: 0px;padding-left: 25px;}
input[type="radio"] +label{font-weight: normal;}

input[type="radio"]+label:before,
input[type="radio"]:checked + label:before{position: absolute;top: 1px;left: 0;width: 15px;height: 15px;content: "";margin: 0px 18px 0 0;}

input[type="radio"]+label {float: left;margin-left: -27px;margin-right: 20px;color: #333333;margin-top: 0px;}

input[type="radio"] + label:before,
input[type="radio"]:checked + label:before{border-radius: 50%;-webkit-box-shadow: 0px 0px 0.2px 1px grey;-moz-box-shadow: 0px 0px 0.2px 1px grey;-ms-box-shadow: 0px 0px 0.2px 1px grey;-o-box-shadow: 0px 0px 0.2px 1px grey;box-shadow: 0px 0px 0.2px 1px grey;}

input[type="radio"] + label:before{background-color: white;}
input[type="radio"]:checked + label:before {background-color: #00AEC7;border: 4px solid #FAFCFE;}

input[type="radio"]+label.custom-label{margin-right:0;}


/* Custom checkbox styles here */
input[type="checkbox"]{ float: left;width: 20px;opacity: 0;height: 20px;position: absolute;filter: alpha(opacity = 0);-khtml-opacity: 0;-moz-opacity: 0;z-index: 1;}
input[type="checkbox"] + label{display: inline-block;float: left;position: relative;padding-top: 0px;padding-left: 25px;font-weight:normal;}
input[type="checkbox"]+label{margin: 0 5px;padding-left: 25px;font-size: 14px;}

input[type="checkbox"]+label:before,
input[type="checkbox"]:checked+label:before {position: absolute;top: 1px;left: 0;width: 18px;height: 18px;content: "";margin: 0px 18px 0 0;background-color:#f8fcff;}

input[type="checkbox"]+label:before,
input[type="checkbox"]:checked+label:before{border-radius: 5px;border: 1px solid grey;padding: 2px 2px 2px 5px;content: "";}

input[type="checkbox"]:checked+label:after{content: " ";position: absolute;display: block;width: 7px;height: 12px;border: solid #00AEC7;border-width: 0 0.25em 0.25em 0;left: 6px;top: 26%;margin-top:2px;-webkit-transform: rotate(45deg);-o-transform: rotate(45deg);transform: rotate(45deg);}


/* Custom Select drop down styles here */
.bootstrap-select>.dropdown-toggle{border-radius: 5px;}
.input-group-btn:last-child>.btn, .input-group-btn:last-child>.btn-group{border-radius: 5px;}
.open>.dropdown-toggle.btn-default{background-color:#fff;border-color:#00AEC7;z-index: 99;}
.open>.dropdown-toggle.btn-default:hover,.bootstrap-select .dropdown-toggle:focus{background-color:#fff;border-color:#00AEC7;z-index: 99;}
.open>.dropdown-toggle.btn-default:focus, .open>.dropdown-toggle.btn-default:hover{background-color:#fff;border-color:#00AEC7;}
.bootstrap-select .dropdown-toggle:focus{outline:0px auto -webkit-focus-ring-color!important;}
.bootstrap-select.btn-group .dropdown-menu{border-radius: 5px;}
.bootstrap-select>.dropdown-toggle{z-index:99;}
.bootstrap-select .btn-default:hover{background-color:#fff;border-color:#00AEC7;}
.btn-group.open .dropdown-toggle,.bootstrap-select .dropdown-menu {box-shadow: none;}
.bootstrap-select.btn-group .dropdown-menu li.selected{border-left:5px solid #00aec7;background:#00aec7;font-weight:bold;}
.dropdown-menu>li>a:hover{background:#68CADA;}
.dropdown-menu>li>a:focus {background:#FFF;}
.bootstrap-select .dropdown-menu{margin:0;z-index:9;}
.bootstrap-select .dropdown-toggle:after{font-family: "Glyphicons Halflings";content: "\e259";}
.bootstrap-select .dropdown-toggle:hover:after{color:#00AEC7;}
.bootstrap-select .dropdown-toggle:focus:after{color:#00AEC7;}
.bootstrap-select .bs-caret{display:none;}
table tr td .bootstrap-select.btn-group:not(.input-group-btn){max-width:7em;}

/* Custom alert messages styles here */
.alert-success{background:#BBE4EC;color:#414042;}
.ok-circle-custom{font-size:40px;float:left;margin-right:.5em;color:#00a0b7;}

.alert-warning{background:#ffe9c7;color:#414042;}
.warning-icon-custom{font-size: 40px;float:left;margin-right:.5em;color:#ffbf3f;}


/*datepicker changes*/
[uib-datepicker-popup-wrap] *:focus{outline:none;}
[uib-datepicker-popup-wrap] .btn-success{background-color: #ccc;border-color: #ccc;color: #000}
[uib-datepicker-popup-wrap] .btn-default{border-color:#fff;}
[uib-datepicker-popup-wrap] .uib-datepicker-current{background-color:#68CADA;}
[uib-datepicker-popup-wrap] .uib-clear{background-color:#da291c;}

.fa-pencil,.fa-close,.fa-check{margin-right:5px;}
.bootstrap-filestyle .form-control[disabled]{background-color: #fff;border-radius: 5px 0 0 5px;}
.bootstrap-filestyle .input-group-btn:last-child>.btn, .input-group-btn:last-child>.btn-group{border-radius: 0 5px 5px 0;}
.bootstrap-filestyle .buttonText{margin-left:5px;}
h3.panel-title{font-weight: bold;}