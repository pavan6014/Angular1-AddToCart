<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <div class="headline"><h2>Product Catalog - PHUB</h2></div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-3">
            <div admin-navigation></div>
        </div>
        <div class="col-sm-9">

            <div class="panel panel-default margin-bottom-40">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa"></i>PHUB</h3>
                </div>
                <div class="panel-body">
                    <div class="row margin-bottom-10">
                        <div class="col-sm-4">
                            <label>Enter BE Code <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control " ng-model="beCode" ng-change="loadProductCatalogSA(beCode)">

                                    <option value="0">Code 1</option>
                                    <option value="1">Code 2</option>
                                    <option value="2">Code 3</option>
                                </select>
                            </div>


                        </div>
                        <div class="row">
                            <div class="col-sm-4 label-bar1">
                                <label>TPIA Code </label>
                            </div>
                        </div>
                    </div>

                    <div class="row margin-bottom-10">
                        <div class="col-sm-4">
                            <label>PHUB Code<span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control" model="productCatalog.servicingArea.pHUBCode" id="pHUBCode" data-ng-model="productCatalog.servicingArea.pHUBCode">
                                    <option value="notselected">Select</option>
                                    <option value="1">PHUB Code1</option>
                                    <option value="2">PHUB Code2</option>
                                    <option value="3">PHUB Code3</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <label>PHUB Description <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <input type="text" class="form-control " placeholder="Description" name="srch-term" id="pHUBDescription" data-ng-model="productCatalog.servicingArea.pHUBDescription" value="">
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <label>Province <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control" id="pHUBProvince" model="productCatalog.servicingArea.pHUBProvince" ng-model="productCatalog.servicingArea.pHUBProvince">
                                    <option selected>Select</option>
                                    <option value="1">Province1</option>
                                    <option value="2">Province2</option>
                                    <option value="3">Province3</option>
                                </select>
                            </div>
                        </div>

                    </div>

                    <div class="row margin-bottom-10">
                        <div class="col-sm-6">
                            <label>Feed Type <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control" id="pHUBFeedType" model="productCatalog.servicingArea.pHUBFeedType" ng-model="productCatalog.servicingArea.pHUBFeedType">
                                    <option selected>Select</option>
                                    <option value="1">Coax(Copper)</option>
                                    <option value="2" >Fibre</option>
                                    <option value="3">Both</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <label>POI Type <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control" id="PHUBPoiType" model="productCatalog.servicingArea.PHUBPoiType" ng-model="productCatalog.servicingArea.PHUBPoiType">
                                    <option selected>Select</option>
                                    <option value="1">Aggregated</option>
                                    <option value="2">Disaggregated</option>
                                </select>
                            </div>
                        </div>
                    </div>

                    <div class="row margin-bottom-10">
                        <div class="col-sm-4">
                            <label>Maximum Uplink <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control"  id="PHUBMaximumUplink" model="productCatalog.servicingArea.PHUBMaximumUplink" ng-model="productCatalog.servicingArea.PHUBMaximumUplink">
                                    <option>Select</option>
                                    <option value="1"> 10Mbps</option>
                                    <option value="2">100Mbps</option>
                                    <option value="3">250Mbps</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <label>Maximum Downlink <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control"  id="PHUBMaximumDownlink" model="productCatalog.servicingArea.PHUBMaximumDownlink" ng-model="productCatalog.servicingArea.PHUBMaximumDownlink">
                                    <option>Select</option>
                                    <option value="1"> 10Mbps</option>
                                    <option value="2" >100Mbps</option>
                                    <option value="3">250Mbps</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <label>Serving Area Status<span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control" id="AreaStatus" model="productCatalog.servicingArea.PHUBServingAreaStatus" ng-model="productCatalog.servicingArea.PHUBServingAreaStatus">
                                    <option>Select</option>
                                    <option value='1'>Available</option>
                                    <option value='2' >Not Available</option>
                                </select>
                            </div>
                        </div>

                    </div>

                    <hr class="devider">
                    <div class="row">
                        <section class="col-sm-12">
                            <button class="btn-u btn-u-default">Cancel</button>
                            <button class="btn-u">Submit</button>
                        </section>
                    </div>


                </div>
            </div>
        </div>
    </div>
</div>