<div class="container">
        <div class="row">
            <div class="col-sm-12">
                <div class="headline"><h2>Create new TPIA account</h2></div>
            </div>
        </div>
    <div class="row">
        <div class="col-sm-3">
            <div admin-navigation></div>
        </div>


        <div class="col-sm-9">

            <div class="panel panel-default margin-bottom-40">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa"></i>Create new TPIA account</h3>
                </div>

                <div class="panel-body">
                    <div>
                         <div class="row margin-bottom-10">
                            <div class="col-sm-6">
                                <label>ISP Name<span style="color: red;">*</span></label>
                                <div class="input-group col-sm-12">
                                    <input type="text" class="form-control" >
                                </div>
                            </div>

                            <div class="col-sm-6">
                                <label>BE Code<span style="color: red;">*</span></label>
                                <div class="input-group col-sm-12">
                                    <input type="text" class="form-control" >
                                </div>
                            </div>
                        </div>

                        <div class="row margin-bottom-10">
                            <div class="col-sm-6">
                                <label>EISP distribution email address<span style="color: red;">*</span></label>
                                <div class="input-group col-sm-12">
                                    <input type="text" class="form-control" >
                                </div>
                            </div>

                        </div>

                    </div>

                    <hr class="devider">
                    <div class="row">
                        <section class="col-sm-12">
                            <button class="btn-u btn-u-default">Cancel</button>
                            <button class="btn-u">Submit</button>
                        </section>
                    </div>
                 </div>

            </div>

            </div>



        </div>
    </div>
</div>