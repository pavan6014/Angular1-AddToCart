                            <div class="input-group">
                                <a ng-show="(queryBy.searchOption === 'postalCode')" href="search/new" class="btn-u ng-hide"><i class="fa fa-add"></i> Continue</a>
                                <a ng-show="(queryBy.searchOption === 'tpiaNo')" href="search/existing" class="btn-u"><i class="fa fa-add"></i> Continue</a>
                                <a ng-show="(queryBy.searchOption === 'transfer')" href="search/transfer" class="btn-u ng-hide"><i class="fa fa-add"></i> Continue</a>
                                <a ng-show="(queryBy.searchOption === 'move')" href="search/move" class="btn-u ng-hide"><i class="fa fa-add"></i> Continue</a>
                                <a ng-show="(queryBy.searchOption === 'misc')" href="search/misc" class="btn-u ng-hide"><i class="fa fa-add"></i> Continue</a>
                            </div>
