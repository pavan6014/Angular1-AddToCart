                    <input type="radio" id="radio01" name="searchOption" class="custom-radio ng-pristine ng-untouched ng-valid" ng-model="queryBy.searchOption" value="tpiaNo">
                    <label>Existing Customer</label>
                    <div class="input-group visible-lg col-md-6">
