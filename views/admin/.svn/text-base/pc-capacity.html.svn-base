<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <div class="headline"><h2>Product Catalog - Capacity</h2></div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-3">
            <div admin-navigation></div>
        </div>
        <div class="col-sm-9">

            <div class="panel panel-default margin-bottom-40">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa"></i>Capacity</h3>
                </div>
                <div class="panel-body">

                    <div class="row margin-bottom-10">
                        <div class="col-sm-4">
                            <label>Serving Area <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control">
                                    <option selected>Select</option>
                                    <option > Active</option>
                                    <option >Expired</option>
                                    <option >Grandfathered</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <label>Serving Area Type<span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control">
                                    <option selected>Select</option>
                                    <option > Active</option>
                                    <option >Expired</option>
                                    <option >Grandfathered</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <label> Link ID <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <input type="text" class="form-control" >
                            </div>
                        </div>

                    </div>

                    <div class="row margin-bottom-10">
                        <div class="col-sm-4">
                            <label>	Maximum Link Capacity  <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control">
                                    <option selected>Select</option>
                                    <option > 1 GB</option>
                                    <option > 10 GB</option>
                                    <option > 100 GB</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <label>	Link Turn-up date <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <input type="text" class="form-control" >
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <label>	Link Status <span style="color: red;">*</span></label>
                            <div class="input-group col-sm-12">
                                <select custom-select  class="form-control">
                                    <option selected>Select</option>
                                    <option > Active</option>
                                    <option >Expired</option>
                                    <option >Grandfathered</option>
                                </select>
                            </div>
                        </div>

                    </div>
                            <hr class="devider">
                            <div class="row">
                                <section class="col-sm-12">
                                    <button class="btn-u btn-u-default">Submit</button>
                                    <button class="btn-u">Save</button>
                                </section>
                            </div>

                </div>
            </div>
        </div>
    </div>
</div>