<div class="container">
	<div class="row">
		<div class="col-sm-12">
			<div class="headline"><h2>Submit New Install Request</h2></div>
		</div>
	</div>
	<div class="row">

		<div class="col-sm-9">
			<div class="row">
				<div class="col-sm-12">
						<div class="">
							<div class="row margin-bottom-20">
								<div class="row">
									<section class="col col-md-9">
										<div class="col-sm-3">
											<label class="label-bar" style="">TPIA Account# </label>
										</div>
										<div class="col-sm-3">
											<input type="text" class="form-control">

										</div>
									</section>
								</div>
							</div>

						</div>


					<div class="row">

						<div class="col-sm-12">
							<div class="panel panel-default">
								<div class="panel-heading">
									<h3 class="panel-title">
										<i class="fa"></i>Customer Details
									</h3>
								</div>
								<div class="panel-body">
									<div class="row margin-bottom-10">
										<div class="col-sm-6">
											<label>Customer Type <span style="color: red;"></span></label>
											<select class="form-control select-drop" custom-select >
												<option selected="" value="TPIA Ba">Residential</option>
												<option value="TPIA Ba2">Office</option>
												<option value="TPIA Ba3">Corporate</option>

											</select>
										</div>

									</div>
									<hr class="devider">

									<div class="row margin-bottom-10">
										<div class="col-sm-6">
											<label>First Name <span style="color: red;">*</span></label>
											<input type="text" class="form-control">
										</div>
										<div class="col-sm-6">
											<label>Last Name <span style="color: red;">*</span></label>
											<input type="text" class="form-control">
										</div>
									</div>
									<div class="row margin-bottom-10">
										<div class="col-sm-4">
											<label>Street Number <span style="color: red;">*</span></label>
											<input type="text" class="form-control">
										</div>
										<div class="col-sm-4">
											<label>Street Name <span style="color: red;">*</span></label>
											<input type="text" class="form-control">
										</div>
										<div class="col-sm-4">
											<label>Street Type <span style="color: red;">*</span></label>
											<input type="text" class="form-control">
										</div>
									</div>

									<div class="row margin-bottom-10">
										<div class="col-sm-4">
											<label>Suite/Unit# <span style="color: red;"></span></label>
											<input type="text" class="form-control">
										</div>
										<div class="col-sm-4">
											<label>Buzz Code </label>
											<input type="text" class="form-control">
										</div>
										<div class="col-sm-4">
											<label>Postal Code <span style="color: red;">*</span></label>
											<input type="text" class="form-control" ng-model="queryBy.postalCode" >
										</div>
									</div>

									<div class="row margin-bottom-10">
										<div class="col-sm-6">
											<label>City/Municipality <span style="color: red;">*</span></label>
											<input type="text" class="form-control">
										</div>
										<div class="col-sm-6">
											<label>Province <span style="color: red;">*</span></label>
											<div class="input-group col-sm-12">
												<select custom-select  class="form-control">
													<option selected>Select</option>
												</select>
											</div>
										</div>
									</div>
									<div class="row margin-bottom-10">
										<div class="col-sm-4">
											<label>Residential Phone# (Non VOIP) <span style="color: red;">*</span></label>
											<input type="text" class="form-control">
										</div>
										<div class="col-sm-8">
											<label>Nearest Main Intersection</label>
											<input type="text" class="form-control">
										</div>
									</div>
								</div>
							</div>
						</div>

						<div class="col-sm-12">
							<div class="panel panel-default" >
								<div class="panel-heading">
									<h3 class="panel-title"><i class="fa"></i> Modem Details</h3>
								</div>
								<div class="panel-body">
									<form  style="border:none;">
										<div class="row">
											<div class="col-sm-6">
												<label>Modem Model<span style="color: red;">*</span></label>
												<div class="input-group col-sm-12">
													<select custom-select  class="form-control">
														<option selected="" value="Select">MDM843982984584</option>
														<option value="Cisco">Cisco</option>
														<option value="Netgear">Netgear</option>
														<option value="Belkin">Belkin</option>
													</select>
												</div>

												<div class="col-12">
													<label class="margin-top-10">Service Type <span style="color: red;">*</span></label>
													<div class="input-group col-sm-12">
														<select custom-select  class="form-control">
															<option selected="" value="Select">15Mbps/1Mbps</option>
															<option value="Cisco">25Mbps/1Mbps</option>
															<option value="Netgear">50Mbps/1Mbps</option>
															<option value="Belkin">100Mbps/1Mbps</option>
														</select>
													</div>
												</div>
											</div>

											<div class="col-sm-6">
												<label>Modem MAC<span style="color: red;">*</span></label>
												<div class="input-group col-sm-12">
													<input type="text" class="form-control" placeholder="MAC98573498753">
												</div>


											</div>
											<div class="col-sm-6">
												<label class="margin-top-10">Modem ESN <span style="color: red;">*</span></label>
												<div class="input-group col-sm-12">
													<input type="text" class="form-control" placeholder="ESN98573498753" >
												</div>

											</div>
										</div>
									</form>
								</div>
							</div>
						</div>


					</div>

					<div class="panel panel-default">
						<div class="panel-heading">
							<h3 class="panel-title"><i class="fa"></i>Appointment Schedule</h3>
						</div>
						<div class="panel-body">
							<form  style="border:none;">
								<div class="row">
									<div class="col-sm-3">
										<label class="padding-top-40">Schedule Date <span style="color:red;"></span></label>
									</div>
									<div class="col-sm-3">
										<label class="margin-top-10">Option 1 <span style="color:red;">*</span></label>
										<div class="input-group col-sm-12">
											<p class="input-group">
												<input type="text" class="form-control radius-left-top-bot" uib-datepicker-popup="{{format}}" ng-model="dt" is-open="popup1.opened" min-date="minDate" max-date="maxDate" datepicker-options="dateOptions" date-disabled="disabled(date, mode)" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" />
                                          <span class="input-group-btn">
                                            <button type="button" class="btn btn-default radius-right-top-bot" ng-click="openDate()"><i class="glyphicon glyphicon-calendar"></i></button>
                                          </span>
											</p>
										</div>


									</div>

									<div class="col-sm-3">
										<label class="margin-top-10">Option 2 <span style="color:red;">*</span></label>
										<div class="input-group col-sm-12">
											<p class="input-group">
												<input type="text" class="form-control radius-left-top-bot" uib-datepicker-popup="{{format}}" ng-model="dt" is-open="popup1.opened" min-date="minDate" max-date="maxDate" datepicker-options="dateOptions" date-disabled="disabled(date, mode)" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" />
                                          <span class="input-group-btn">
                                            <button type="button" class="btn btn-default radius-right-top-bot" ng-click="openDate()"><i class="glyphicon glyphicon-calendar"></i></button>
                                          </span>
											</p>
										</div>


									</div>
									<div class="col-sm-3">
										<label class="margin-top-10">Option 3 <span style="color:red;">*</span></label>
										<div class="input-group col-sm-12">
											<p class="input-group">
												<input type="text" class="form-control radius-left-top-bot" uib-datepicker-popup="{{format}}" ng-model="dt" is-open="popup1.opened" min-date="minDate" max-date="maxDate" datepicker-options="dateOptions" date-disabled="disabled(date, mode)" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" />
                                          <span class="input-group-btn">
                                            <button type="button" class="btn btn-default radius-right-top-bot" ng-click="openDate()"><i class="glyphicon glyphicon-calendar"></i></button>
                                          </span>
											</p>
										</div>


									</div>
								</div>

								<div class="row margin-top-10">
									<div class="col-sm-3">
										<label class="label-bar">Preferred Window</label>
									</div>
									<div class="col-sm-3">
										<select custom-select  class="form-control">
											<option selected="" value="Select">Rogers</option>
											<option value="Cisco"></option>
											<option value="Cisco"></option>
											<option value="Cisco"></option>
										</select>

									</div>
								</div>
								<hr class="devider">
								<div class="row">
									<section class="col-md-12">
										<button class="btn-u btn-u-default">Cancel</button>
										<button class="btn-u">Submit</button>
									</section>
								</div>

							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>